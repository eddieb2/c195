   <children>
      <TableView fx:id="appointmentsTable" editable="true" prefHeight="800.0" prefWidth="1200.0">
         <columns>
            <TableColumn fx:id="idCol" prefWidth="75.0" text="Apt. ID" />
            <TableColumn fx:id="titleCol" prefWidth="75.0" text="Title" />
