<!--            <TableColumn fx:id="createdByCol" prefWidth="75.0" text="Created By" />-->
<!--            <TableColumn fx:id="lastUpdateCol" prefWidth="89.0" text="Last Update" />-->
<!--            <TableColumn fx:id="lastUpdatedByCol" prefWidth="82.0" text="Last Updated By" />-->
            <TableColumn fx:id="customerIdCol" prefWidth="75.0" text="Customer ID" />
            <TableColumn fx:id="userIdCol" prefWidth="75.0" text="User ID" />
            <TableColumn fx:id="contactIdCol" prefWidth="75.0" text="Contact ID" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
